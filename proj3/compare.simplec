comparisons(x, y, z) : function(int, int, int) -> int {
  if (x != 0 && y != 0 && z != 0) {
    if (x < y) {
        if (x <= 5) {
          x = (x + 5);
        }
      }
    if (z >= y) {
      if (x == y) {
        x = (x - z);
      }
    }
  }

  if (x == 0 || y == 0 || z == 0) {
    x = 1;
    y = 2;
    z = 3;
  }

  return 0;
}


main { 
  int a;
  int b;
  int c;
  int d;
  a = 5;
  b = 10;
  c = 15;
  d = comparisons(a, b, c);
  return 0;
}
