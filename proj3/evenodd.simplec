isodd(x) : function(int) -> int {
	int comp;
	if (x % 2 == 1) {
		comp =  1;
	} else {
		comp = 0;
	}
	return comp;
}

iseven(x) : function(int) -> int {
	int comp;
	if (x % 2 != 1) {
		comp = 1;
	} else {
		comp = isodd(x);
	}
	return comp;
}

main {

	int x;
	int fifo;
	x = 3;
	while (10 >= x) {
		fifo = isodd(x);
		fifo = iseven(x);
		x = x + 1;
	}
	return 0;
}