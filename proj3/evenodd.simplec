isodd(x, y, z) : function(int, int, int) -> int {
	int comp;
	if (x % z == y) {
		comp =  1;
	} else {
		comp = 0;
	}
	return comp;
}

iseven(x, y, z) : function(int, int, int) -> int {
	int comp;
	if (x % z != y) {
		comp = 1;
	} else {
		comp = isodd(x, y, z);
	}
	return comp;
}

main {
	int x;
	int y;
	int z;
	int fifo;
	x = 0;
	y = 1;
	z = 2;
	while (10 >= x) {
		fifo = isodd(x, y, z);
		fifo = iseven(x, y, z);
		x = x + y;
	}
	return 0;
}